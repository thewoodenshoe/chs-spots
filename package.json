{
  "name": "chs-spots",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 jest",
    "test:incremental": "jest __tests__/seed-incremental.test.js",
    "test:pipeline": "node scripts/__tests__/validate-pipeline.js",
    "test:pipeline:data": "node scripts/__tests__/validate-data-structures.js",
    "test:pipeline:integration": "jest scripts/__tests__/pipeline-integration.test.js",
    "test:bulk-process": "node scripts/__tests__/validate-process-bulk-llm-results.js",
    "test:create-spots": "node scripts/__tests__/validate-create-spots.js",
    "extract:bulk:process": "node scripts/process-bulk-llm-results.js",
    "extract:incremental": "node scripts/extract-promotions.js --incremental",
    "seed:incremental": "node scripts/seed-incremental.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "jest --passWithNoTests --findRelatedTests"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "jest --passWithNoTests --findRelatedTests"
    ]
  },
  "dependencies": {
    "@react-google-maps/api": "^2.19.4",
    "cheerio": "^1.1.2",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "node-fetch": "^3.3.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
